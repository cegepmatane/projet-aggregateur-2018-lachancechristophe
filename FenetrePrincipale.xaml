<Window x:Class="TP2_ProjetAgregateur.FenetrePrincipale"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TP2_ProjetAgregateur"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnPokeapi" Content="PokeAPI" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,10,0,0" Click="btnPokeapi_Click"/>
        <Button x:Name="btnCrypto" Content="Crypto" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,35,0,0" Click="btnCrypto_Click"/>
        <Button x:Name="btnVulnerabilite" Content="Vulnérabilités" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,60,0,0" Click="btnVulnerabilite_Click"/>
        <Button x:Name="btnSlack" Content="Slack" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,110,0,0" Click="btnSlack_Click"/>
        <Button x:Name="btnSeismes" Content="Seismes" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="10,85,0,0" Click="btnSeismes_Click"/>

    </Grid>
</Window>
