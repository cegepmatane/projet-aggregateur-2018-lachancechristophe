<Window x:Class="TP2_ProjetAgregateur.VueSlack"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TP2_ProjetAgregateur"
        mc:Ignorable="d"
        Title="VueSlack" Height="450" Width="357.022">
    <Grid>
        <Button x:Name="btnChargerSalons" Content="Charger Salons" HorizontalAlignment="Left" VerticalAlignment="Top" Width="104" Margin="237,36,0,0" Click="actionChargerSalons"/>
        <Label x:Name="lblSalons" Content="Salons :" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"/>
        <ListBox x:Name="lstSalons" HorizontalAlignment="Left" Height="100" VerticalAlignment="Top" Width="222" Margin="10,36,0,0"/>
        <Button x:Name="btnChargerMessages" Content="Charger Messages" HorizontalAlignment="Left" VerticalAlignment="Top" Width="104" Margin="237,117,0,0" Click="actionChargerMessages"/>
        <Label x:Name="lblMessages" Content="Messages :" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,141,0,0"/>
        <ListBox x:Name="lstMessages" HorizontalAlignment="Left" Height="243" VerticalAlignment="Top" Width="331" Margin="10,167,0,0"/>
    </Grid>
</Window>
